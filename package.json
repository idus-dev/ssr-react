{
  "name": "ssr-react",
  "version": "1.0.0",
  "description": "ssr-react",
  "keywords": [
    "boilerplate",
    "ssr",
    "react"
  ],
  "author": "tylee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/7ylee/ssr-react.git"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "webpack:dev": "rm -rf ./dev && webpack -w --mode development --config webpack.config.js",
    "production": "NODE_ENV=production node scripts/start.js",
    "development": "npm run webpack:dev & NODE_ENV=development babel-node scripts/start.js",
    "build": "rm -rf ./build && webpack --config webpack.prod.js",
    "docker:build": "docker build --rm -t tylee/ssr-react .",
    "docker:start": "docker run --rm -it -p 80:8080 tylee/ssr-react",
    "docker": "npm run docker:build && npm run docker:start"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/runtime": "^7.9.2",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "body-parser": "^1.19.0",
    "chokidar": "^3.3.1",
    "compression-webpack-plugin": "^3.1.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-7ylee": "0.0.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "git-branch": "^2.0.1",
    "html-webpack-change-assets-extension-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "morgan": "^1.10.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.1",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-pwa-manifest": "^4.2.0",
    "winston": "^3.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      [
        "styled-components",
        {
          "displayName": false,
          "ssr": true,
          "minify": true,
          "pure": true
        }
      ]
    ]
  },
  "proxy": "//localhost:3000"
}
